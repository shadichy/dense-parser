{
  "name": "dense-parser",
  "version": "1.0.2",
  "description": "Dense.js JSON-HTML parser script",
  "main": "./dist/parser.cjs.js",
  "module": "./dist/parser.es.js",
  "scripts": {
    "build": "rollup -c rollup.config.mjs && rollup -c rollup.config.umd.mjs",
    "watch": "rollup -wc rollup.config.mjs && rollup -wc rollup.config.umd.mjs",
    "prepare": "eslint ./src/parser.js && prettier --write ./src/parser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadichy/dense-parser.git"
  },
  "keywords": [
    "dense",
    "parser"
  ],
  "author": "Shadichy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadichy/dense-parser/issues"
  },
  "homepage": "https://github.com/shadichy/dense-parser#readme",
  "dependencies": {
    "emmet": "^2.3.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "emmet": "^2.3.6",
    "eslint": "^8.25.0",
    "rollup": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "prettier": "^2.7.1"
  },
  "directories": {
    "example": "example"
  }
}
